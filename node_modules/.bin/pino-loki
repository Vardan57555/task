#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbook/Downloads/Task/node_modules/.pnpm/pino-loki@2.6.0/node_modules/pino-loki/dist/node_modules:/Users/macbook/Downloads/Task/node_modules/.pnpm/pino-loki@2.6.0/node_modules/pino-loki/node_modules:/Users/macbook/Downloads/Task/node_modules/.pnpm/pino-loki@2.6.0/node_modules:/Users/macbook/Downloads/Task/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbook/Downloads/Task/node_modules/.pnpm/pino-loki@2.6.0/node_modules/pino-loki/dist/node_modules:/Users/macbook/Downloads/Task/node_modules/.pnpm/pino-loki@2.6.0/node_modules/pino-loki/node_modules:/Users/macbook/Downloads/Task/node_modules/.pnpm/pino-loki@2.6.0/node_modules:/Users/macbook/Downloads/Task/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/pino-loki@2.6.0/node_modules/pino-loki/dist/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/pino-loki@2.6.0/node_modules/pino-loki/dist/cli.js" "$@"
fi
